  <g filter="url(#filter0_b_2_20)">
    <path d="M587 -469.097C587 -324.452 349.352 83 64.6495 83C-220.053 83 -444 -324.452 -444 -469.097C-444 -613.742 -213.203 -731 71.5 -731C356.203 -731 587 -613.742 587 -469.097Z" fill="url(#paint0_linear_2_20)" fill-opacity="0.6"/>
  </g>
  <defs>
    <filter id="filter0_b_2_20" x="-448" y="-735" width="1039" height="822" filterUnits="userSpaceOnUse" color-interpolation-filters="sRGB">
      <feFlood flood-opacity="0" result="BackgroundImageFix"/>
      <feGaussianBlur in="BackgroundImageFix" stdDeviation="2"/>
      <feComposite in2="SourceAlpha" operator="in" result="effect1_backgroundBlur_2_20"/>
